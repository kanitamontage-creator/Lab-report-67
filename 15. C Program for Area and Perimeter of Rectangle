#include <stdio.h>

int main() {
    float length, width, area, perimeter;
    printf("Enter length and width of the rectangle: ");
    scanf("%f %f", &length, &width);

    area = length * width;
    perimeter = 2 * (length + width);

    printf("Area = %f\n", area);
    printf("Perimeter = %f", perimeter);

    return 0;
}